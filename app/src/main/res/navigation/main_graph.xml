<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/FBase">

    <fragment
        android:id="@+id/FBase"
        android:name="com.upd.kventas.ui.fragment.FBase"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_f_base">
        <action
            android:id="@+id/action_FBase_to_FAjuste"
            app:destination="@id/FAjuste"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_FBase_to_DLogin"
            app:destination="@id/DLogin"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_FBase_to_FCliente"
            app:destination="@id/FCliente"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_FBase_to_FReporte"
            app:destination="@id/FReporte"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_FBase_to_FVendedor"
            app:destination="@id/FVendedor"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_FBase_to_FRastreo"
            app:destination="@id/FRastreo"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_FBase_to_FAlta"
            app:destination="@id/FAlta"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_FBase_to_FBaja"
            app:destination="@id/FBaja"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/FAjuste"
        android:name="com.upd.kventas.ui.fragment.FAjuste"
        android:label="Ajustes"
        tools:layout="@layout/fragment_f_ajuste" >
    </fragment>
    <dialog
        android:id="@+id/DLogin"
        android:name="com.upd.kventas.ui.dialog.BDLogin"
        android:label="Login"
        tools:layout="@layout/bottom_dialog_login">
        <action
            android:id="@+id/action_DLogin_to_FAjuste"
            app:destination="@id/FAjuste"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </dialog>
    <fragment
        android:id="@+id/FCliente"
        android:name="com.upd.kventas.ui.fragment.FCliente"
        android:label="Clientes"
        tools:layout="@layout/fragment_f_cliente" >
        <action
            android:id="@+id/action_FCliente_to_FMapa"
            app:destination="@id/FMapa"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_FCliente_to_DObservacion"
            app:destination="@id/DObservacion"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_FCliente_to_DBaja"
            app:destination="@id/DBaja"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/FMapa"
        android:name="com.upd.kventas.ui.fragment.FMapa"
        android:label="Mapa"
        tools:layout="@layout/fragment_f_mapa">
        <action
            android:id="@+id/action_FMapa_to_DObservacion"
            app:destination="@id/DObservacion"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_FMapa_to_DBaja"
            app:destination="@id/DBaja"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <dialog
        android:id="@+id/DObservacion"
        android:name="com.upd.kventas.ui.dialog.DObservacion"
        android:label="Observacion"
        tools:layout="@layout/dialog_observacion">
        <action
            android:id="@+id/action_DObservacion_to_FEncuesta"
            app:destination="@id/FEncuesta"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <argument
            android:name="cliente"
            android:defaultValue=''
            app:argType="string" />
    </dialog>
    <fragment
        android:id="@+id/FEncuesta"
        android:name="com.upd.kventas.ui.fragment.FEncuesta"
        android:label="Encuesta"
        app:enterAnim="@anim/slide_in_right"
        app:exitAnim="@anim/slide_out_left"
        app:popEnterAnim="@anim/slide_in_left"
        app:popExitAnim="@anim/slide_out_right"
        tools:layout="@layout/fragment_f_encuesta" />
    <dialog
        android:id="@+id/DBaja"
        android:name="com.upd.kventas.ui.dialog.DBaja"
        android:label="Baja"
        tools:layout="@layout/dialog_baja">
        <argument
            android:name="cliente"
            android:defaultValue=''
            app:argType="string" />
    </dialog>
    <fragment
        android:id="@+id/FReporte"
        android:name="com.upd.kventas.ui.fragment.FReporte"
        android:label="Reporte"
        tools:layout="@layout/fragment_f_reporte" >
        <action
            android:id="@+id/action_FReporte_to_FDetalle"
            app:destination="@id/FDetalle"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_FReporte_to_DMiniDetalle"
            app:destination="@id/DMiniDetalle"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/FDetalle"
        android:name="com.upd.kventas.ui.fragment.FDetalle"
        android:label="Detalle"
        tools:layout="@layout/fragment_f_detalle">
        <argument
            android:name="ume"
            app:argType="com.upd.kventas.data.model.Umes"
            app:nullable="true"/>
        <argument
            android:name="soles"
            app:argType="com.upd.kventas.data.model.Soles"
            app:nullable="true"/>
        <argument
            android:name="visisuper"
            app:argType="com.upd.kventas.data.model.Visisuper[]"
            app:nullable="true"/>
        <action
            android:id="@+id/action_FDetalle_to_DMiniDetalle"
            app:destination="@id/DMiniDetalle"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <dialog
        android:id="@+id/DMiniDetalle"
        android:name="com.upd.kventas.ui.dialog.DMiniDetalle"
        android:label="dialog_mini_detalle"
        tools:layout="@layout/dialog_mini_detalle" >
        <argument
            android:name="preventa"
            app:argType="com.upd.kventas.data.model.Volumen[]"
            app:nullable="true" />
        <argument
            android:name="cobertura"
            app:argType="com.upd.kventas.data.model.CobCart[]"
            app:nullable="true"/>
        <argument
            android:name="cartera"
            app:argType="com.upd.kventas.data.model.CobCart[]"
            app:nullable="true"/>
        <argument
            android:name="cambios"
            app:argType="com.upd.kventas.data.model.Cambio[]"
            app:nullable="true"/>
        <argument
            android:name="visicooler"
            app:argType="com.upd.kventas.data.model.Visicooler[]"
            app:nullable="true"/>
        <argument
            android:name="generico"
            app:argType="com.upd.kventas.data.model.Generico"
            app:nullable="true"/>
        <argument
            android:name="umes"
            app:argType="com.upd.kventas.data.model.Umes"
            app:nullable="true" />
        <argument
            android:name="soles"
            app:argType="com.upd.kventas.data.model.Soles"
            app:nullable="true" />
        <argument
            android:name="pedidos"
            app:argType="integer"
            android:defaultValue="0" />
    </dialog>
    <fragment
        android:id="@+id/FVendedor"
        android:name="com.upd.kventas.ui.fragment.FVendedor"
        android:label="Clientes"
        tools:layout="@layout/fragment_f_vendedor">
        <action
            android:id="@+id/action_FVendedor_to_DObservacion"
            app:destination="@id/DObservacion"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_FVendedor_to_DBaja"
            app:destination="@id/DBaja"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_FVendedor_to_FMapa"
            app:destination="@id/FMapa"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/FRastreo"
        android:name="com.upd.kventas.ui.fragment.FRastreo"
        android:label="Monitoreo"
        tools:layout="@layout/fragment_f_rastreo" />
    <fragment
        android:id="@+id/FAlta"
        android:name="com.upd.kventas.ui.fragment.FAlta"
        android:label="Alta"
        tools:layout="@layout/fragment_f_alta" >
        <action
            android:id="@+id/action_FAlta_to_FAltaMapa"
            app:destination="@id/FAltaMapa"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_FAlta_to_FAltaDatos"
            app:destination="@id/FAltaDatos"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/FAltaMapa"
        android:name="com.upd.kventas.ui.fragment.FAltaMapa"
        android:label="Alta manual"
        tools:layout="@layout/fragment_f_alta_mapa" >
        <action
            android:id="@+id/action_FAltaMapa_to_FAltaDatos"
            app:destination="@id/FAltaDatos"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <argument
            android:name="idaux"
            app:argType="integer"
            android:defaultValue="0" />
    </fragment>
    <fragment
        android:id="@+id/FAltaDatos"
        android:name="com.upd.kventas.ui.fragment.FAltaDatos"
        android:label="Datos del alta"
        tools:layout="@layout/fragment_f_alta_datos" >
        <action
            android:id="@+id/action_FAltaDatos_to_FAlta"
            app:popUpTo="@+id/FAlta"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_FAltaDatos_to_FAltaMapa"
            app:destination="@id/FAltaMapa"
            app:popUpTo="@+id/FAlta"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <argument
            android:name="idaux"
            app:argType="integer"
            android:defaultValue="0" />
    </fragment>
    <fragment
        android:id="@+id/FBaja"
        android:name="com.upd.kventas.ui.fragment.FBaja"
        android:label="Baja"
        tools:layout="@layout/fragment_f_baja" >
        <action
            android:id="@+id/action_FBaja_to_FBajaDatos"
            app:destination="@id/FBajaDatos"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/FBajaDatos"
        android:name="com.upd.kventas.ui.fragment.FBajaDatos"
        android:label="Bajas generadas"
        tools:layout="@layout/fragment_f_baja_datos" >
        <action
            android:id="@+id/action_FBajaDatos_to_FValidar"
            app:destination="@id/FValidar"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/FValidar"
        android:name="com.upd.kventas.ui.fragment.FValidar"
        android:label="Validar"
        tools:layout="@layout/fragment_f_validar" >
        <action
            android:id="@+id/action_FValidar_to_FBajaDatos"
            app:destination="@id/FBajaDatos"
            app:popUpTo="@+id/FBaja"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <argument
            android:name="datos"
            app:argType="string"
            app:nullable="true"/>
    </fragment>
</navigation>